{
	"name": "@shipos/database",
	"version": "0.0.0",
	"private": true,
	"main": "./index.ts",
	"types": "./**/.tsx",
	"scripts": {
		"db:generate": "prisma generate --no-hints --schema=./prisma/schema.prisma",
		"db:push": "prisma db push --skip-generate --schema=./prisma/schema.prisma",
		"db:migrate": "prisma migrate dev --schema=./prisma/schema.prisma",
		"db:studio": "prisma studio --schema=./prisma/schema.prisma",
		"type-check": "tsc --noEmit"
	},
	"dependencies": {
		"@paralleldrive/cuid2": "^2.2.2",
		"@prisma/client": "^6.15.0",
		"@shipos/config": "workspace:*",
		"zod": "^4.1.5"
	},
	"devDependencies": {
		"@repo/tsconfig": "workspace:*",
		"@types/node": "^24.3.1",
		"prisma": "^6.15.0",
		"prisma-zod-generator": "^1.18.5",
		"typescript": "5.9.2"
	}
}
